{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "semicolon": [true, "always"],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "no-console": false,
        "whitespace": [true, "check-module"]
    },
    "rulesDirectory": []
}